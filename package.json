{
  "name": "views.thenew",
  "version": "1.0.0",
  "description": "thenew views - blog",
  "scripts": {
    "css": "grunt watch",
    "serve": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --port=1212",
    "serve:prod": "ELEVENTY_ENV=production npx @11ty/eleventy --serve --port=1212",
    "dev": "yarn serve",
    "dev:prod": "yarn serve:prod",
    "build": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "watch": "npx @11ty/eleventy --watch",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "images": "pngquant --ext .jpg 3 assets/to/*",
    "images4": "pngquant --ext 4.jpg 4 assets/to/*",
    "images2": "pngquant --ext 2.jpg 2 assets/to/*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thenew/views.git"
  },
  "author": {
    "name": "thenew",
    "url": "http://thenew.fr"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "grunt": "^1.5.2",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-contrib-stylus": "^1.2.0",
    "grunt-contrib-watch": "^1.1.0",
    "load-grunt-tasks": "^4.0.0",
    "luxon": "^2.3.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-link-attributes": "^4.0.0",
    "pngquant": "^3.0.0"
  }
}
